{
  "name": "yellow_first",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:dev": "cross-env NODE_ENV=dev node dist/src/main",
    "start:staging": "cross-env NODE_ENV=staging node dist/src/main",
    "start:prod": "cross-env NODE_ENV=prod node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/database/orm.config.ts",
    "migrations:generate:local": "cross-env NODE_ENV=local npm run typeorm -- migration:generate src/database/migrations/local/%npm_config_name%",
    "migrations:run:local": "cross-env NODE_ENV=local npm run typeorm migration:run --",
    "migrations:revert:local": "cross-env NODE_ENV=local npm run typeorm -- migration:revert",
    "migrations:show:local": "cross-env NODE_ENV=local npm run typeorm -- migration:show",
    "seed:run:local": "cross-env NODE_ENV=local ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/orm.config.ts",
    "migrations:generate:dev": "cross-env NODE_ENV=dev npm run typeorm -- migration:generate src/database/migrations/dev/%npm_config_name%",
    "migrations:run:dev": "cross-env NODE_ENV=dev npm run typeorm migration:run --",
    "migrations:revert:dev": "cross-env NODE_ENV=dev npm run typeorm -- migration:revert",
    "migrations:show:dev": "cross-env NODE_ENV=dev npm run typeorm -- migration:show",
    "seed:run:dev": "cross-env NODE_ENV=dev ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/orm.config.ts",
    "migrations:generate:prod": "cross-env NODE_ENV=prod npm run typeorm -- migration:generate src/database/migrations/prod/%npm_config_name%",
    "migrations:run:prod": "cross-env NODE_ENV=prod npm run typeorm migration:run --",
    "migrations:revert:prod": "cross-env NODE_ENV=prod npm run typeorm -- migration:revert",
    "migrations:show:prod": "cross-env NODE_ENV=prod npm run typeorm -- migration:show",
    "seed:run:prod": "cross-env NODE_ENV=prod ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/orm.config.ts",
    "prepare": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.4.0",
    "bcrypt": "5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express-basic-auth": "^1.2.1",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.11.1",
    "moment-timezone": "^0.5.45",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-saml": "^3.2.4",
    "pg": "^8.10.0",
    "prom-client": "^15.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.12",
    "typeorm-extension": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.4",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-hexagonal-architecture": "^1.0.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.1.3"
  }
}
